<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="PcpreportPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>pcpreports.Employee</class>
    <class>pcpreports.Importlog</class>
    <class>pcpreports.Location</class>
    <class>pcpreports.Pcp</class>
    <class>pcpreports.Report</class>
    <class>pcpreports.Subunit</class>
    <class>pcpreports.Units</class>
    <class>pcpreports.Setup</class>
    <class>pcpreports.Absensi</class>
    <properties>
      <property name="toplink.jdbc.user" value="app"/>
      <property name="toplink.jdbc.password" value="app"/>
      <property name="toplink.jdbc.url" value="jdbc:derby:C:/Pcpreports/Pcpreport;create=true"/>
      <property name="toplink.jdbc.driver" value="org.apache.derby.jdbc.EmbeddedDriver"/>
      <property name="toplink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
