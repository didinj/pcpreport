CREATE TABLE IMPORTLOG (ID BIGINT NOT NULL, IMPORTDATE VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE LOCATION (LOCCODE VARCHAR(255) NOT NULL, LOCNAME VARCHAR(255), LOCUNIT VARCHAR(255), LOCRFID VARCHAR(255), PRIMARY KEY (LOCCODE))
CREATE TABLE EMPLOYEE (NIK VARCHAR(255) NOT NULL, JABATAN VARCHAR(255), REGU INTEGER, NAMA VARCHAR(255), UNIT VARCHAR(255), RFID VARCHAR(255), PRIMARY KEY (NIK))
CREATE TABLE REPORT (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, REGU INTEGER, TGL DATE, NAMA VARCHAR(255), JAM TIME, NIK VARCHAR(255), LOKASI VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PCP (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, TGL DATE, JAM TIME, NIK VARCHAR(255), LOK VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL, PRIMARY KEY (SEQ_NAME))
